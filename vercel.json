{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build-production",
  "outputDirectory": "frontend-vite-react/dist",
  "installCommand": "npm install",
  "framework": null,
  "headers": [
    {
      "source": "/midnight/counter/keys/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/midnight/counter/zkir/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
